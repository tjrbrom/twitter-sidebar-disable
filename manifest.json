{
    "name": "disable twitter sidebar",
    "version": "1.0.0",
    "description": "well duh",
    "manifest_version": 2,
    
    "permissions": [
        "tabs",
        "notifications",
        "http://*/",
        "https://*/"
    ],

    "icons": {
        "48": "icons/twtr-side.png"
    },
	
    "browser_action": {
	"default_icon": "icons/twtr-side.png",
        "default_title": "disable twitter sidebar"
    },
	
    "background": {
        "scripts": ["scripts/background.js"]
    }

}
